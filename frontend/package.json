{
  "private": true,
  "scripts": {
    "build": "pulp browserify --to dist/app.js",
    "watch": "pulp -w browserify --to dist/app.js",
    "watch-fast": "webpack --watch",
    "browse": "live-server dist --watch *.js,*.css,*.html --ignore .*",
    "dev": "concurrently \"yarn run watch-fast\" \"yarn run browse\""
  },
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.17",
    "concurrently": "^3.5.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "live-server": "^1.2.0",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "aws-sdk": "^2.132.0"
  }
}
